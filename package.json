{
  "name": "canvas-design-blocks",
  "version": "2.14.10",
  "type": "module",
  "license": "GPL-3.0-or-later",
  "size-limit": [
    {
      "name": "Desktop (Initial Load)",
      "brotli": true,
      "path": [
        "dist/canvas-blocks.min.js",
        "dist/canvas-blocks.css",
        "dist/theme-loader.umd.cjs"
      ]
    },
    {
      "name": "Mobile App",
      "brotli": true,
      "path": [
        "dist/canvas-blocks-mobile.min.js",
        "dist/canvas-blocks-mobile.css",
        "dist/theme-loader.umd.cjs"
      ]
    },
    {
      "name": "Full app launch",
      "brotli": true,
      "path": [
        "dist/c-desktop/*",
        "dist/canvas-blocks.min.js",
        "dist/canvas-blocks.css",
        "dist/theme-loader.umd.cjs"
      ]
    }
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build --mode desktop && vite build --mode mobile",
    "build:mobile": "vite build --mode mobile",
    "build:desktop": "vite build --mode desktop",
    "preview": "vite preview",
    "zip": "yarn build-theme && cd ./dist/ && bestzip design-blocks-theme.zip *",
    "build-theme": "yarn build && vite build --mode theme-loader && vite build --mode theme-loader-mobile && node ./scripts/buildTheme.js",
    "icons": "tsx ./src/lib/icons/buildIcons.ts",
    "size": "yarn build-theme && size-limit"
  },
  "devDependencies": {
    "@braintree/sanitize-url": "^7.0.0",
