{
  "name": "canvas-design-blocks",
  "version": "2.9.2",
  "type": "module",
  "license": "GPL-3.0-or-later",
  "size-limit": [
    {
      "name": "Initial Load",
      "brotli": true,
      "path": [
        "dist/canvas-blocks.min.js",
        "dist/theme-loader.umd.cjs"
      ]
    },
    {
      "name": "Full app launch",
      "brotli": true,
      "path": [
        "dist/c/*.js",
        "dist/canvas-blocks.min.js",
        "dist/theme-loader.umd.cjs"
      ]
    }
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "zip": "vite build && cd ./dist/ && bestzip bundle.zip icon/* assets/* src/* canvas-blocks.min.js canvas-grid.umd.cjs favicon.png favicon.svg manifest.json popup.html",
    "zip-testing": "vite build --mode testing && cd ./dist/ && bestzip bundle.zip icon/* assets/* src/* favicon.png favicon.svg manifest.json popup.html",
    "zip-beta": "vite build && cd ./dist/ && mkdir -p pkg && rimraf ./pkg/* && cp -r icon extension-loader.js canvas-blocks.min.js canvas-grid.umd.cjs favicon.png favicon.svg manifest.beta.json popup.html ./pkg && mv ./pkg/manifest.beta.json ./pkg/manifest.json && cd pkg && bestzip bundle.beta.zip ./*",
    "build-theme": "vite build && vite build --mode theme-loader && node ./scripts/buildTheme.js",
    "size": "yarn build-theme && size-limit"
  },
  "devDependencies": {
    "@braintree/sanitize-url": "^6.0.4",
    "@crxjs/vite-plugin": "^2.0.0-beta.17",
    "@floating-ui/dom": "1.5.3",
    "@instructure/ui-icons": "^7",
    "@laynezh/vite-plugin-lib-assets": "^0.5.5",
    "@size-limit/preset-app": "^9.0.0",
    "@sveltejs/vite-plugin-svelte": "^2.4.6",
    "@tailwindcss/forms": "^0.5.6",
    "@tailwindcss/nesting": "^0.0.0-insiders.565cd3e",
    "@types/css": "^0.0.34",
    "@types/gtag.js": "^0.0.16",
    "@types/jsdom": "^21.1.3",
    "@types/node": "^20.7.0",
    "@types/sortablejs": "^1.15.3",
    "@types/to-px": "^1.1.2",
    "autoprefixer": "10.4.16",
    "bestzip": "^2.2.1",
    "changelog-parser": "^3.0.1",
    "colord": "^2.9.3",
    "compare-versions": "^6.1.0",
    "dotenv": "^16.1.4",
    "fast-glob": "^3.3.1",
    "filesize": "^10.0.7",
    "html-void-elements": "3.0.0",
    "jsdom": "^22.1.0",
    "lightningcss": "^1.22.0",
    "magic-string": "^0.30.2",
    "nanoid": "5.0.1",
    "perfect-debounce": "^1.0.0",
    "postcss": "8.4.30",
    "postcss-editor-styles": "^0.3.0",
    "postcss-exclude-files": "^1.0.15",
    "postcss-for": "^2.1.1",
    "postcss-import": "^15.0.0",
    "postcss-load-config": "^4.0.1",
    "postcss-nesting": "12.0.1",
    "rimraf": "5.0.4",
    "rollup-plugin-visualizer": "5.9.2",
    "sass": "1.68.0",
    "size-limit": "^9.0.0",
    "sortablejs": "^1.15.0",
    "svelte": "^4.2.1",
    "svelte-persisted-store": "^0.7.0",
    "svelte-portal": "^2.2.0",
    "svelte-preprocess": "^5.0.3",
    "svelte-use-click-outside": "^1.0.0",
    "svelte-writable-derived": "^3.1.0",
    "tailwindcss": "3.3.3",
    "tinymce": "5.10",
    "to-px": "^1.1.0",
    "typescript": "^5.0.0",
    "vite": "^4.3.9"
  }
}
