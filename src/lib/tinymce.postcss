.canvas-grid-editor {
  & .grid-row {
    @apply flex gap-3;
    @apply p-3 m-3 outline-dashed outline-2 outline-gray-200;
    & [class=^="col-"] {
      @apply p-3 outline-dashed outline-2 outline-uni-color-blue;
    }
  }
}
@for $i from 1 to 12 {
  .canvas-grid-editor .col-md-$i {
    width: calc(100% / 12 * $(i));
  }
}
